ProcessHeaders "<main>main.app" : "$(HEADERS)" : "" : BPTag000 ;

# Resources
RESOURCES = ;
ProcessResources "<main>main.app" : "$(RESOURCES)" : BPTag000 : BPTag001 ;

# Sources
SOURCES = ../sdl_test/main.c ;
ProcessSources "<main>main.app" : "$(SOURCES)" : BPTag001 : BPTag002 ;

# Frameworks & Libraries
FRAMEWORKS_AND_LIBRARIES = ;
ProcessFrameworksAndLibraries "<main>main.app" : "$(FRAMEWORKS_AND_LIBRARIES)" : BPTag002 : BPTag003 ;

# Rez Files
REZ_FILES = ;
ProcessRezFiles "<main>main.app" : "$(REZ_FILES)" : BPTag003 : BPTag004 ;

include $(TEMP_DIR)/HeaderDependencies.jam ;
EndApplication "<main>main.app" ;

