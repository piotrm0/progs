# Target "main"

ConditionalDefinitions _ ;
commitdeferred ;


BeginApplication "<main>main.app" ;

# Headers
/Library/Frameworks/SDL.framework/Versions/A/Headers/SDL.h_ATTRIBUTES = AbsolutePath ;
